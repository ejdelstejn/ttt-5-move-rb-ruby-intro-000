#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

prompt("Welcome to Tic Tac Toe!")

board = [" "," "," "," "," "," "," "," "," "]

display_board(board)

prompt("Where would you like to go?")
input = gets.strip

index = input_to_index(input)

move(board, index, value = 'X')

display_board(board)
